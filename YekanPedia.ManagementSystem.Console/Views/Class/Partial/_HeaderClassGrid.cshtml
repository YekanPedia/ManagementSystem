@model Class
<thead>
    <tr>
        <th data-column-id="@nameof(Model.ClassId)" data-visible="false">
            @Html.DisplayNameFor(X => X.ClassId)
        </th>
        <th data-column-id="@nameof(Model.ClassTypeId)">
            @Html.DisplayNameFor(X => X.ClassTypeId)
        </th>
        <th data-column-id="@nameof(Model.CourseId)">
            @Html.DisplayNameFor(X => X.CourseId)
        </th>
        <th data-column-id="@nameof(Model.UserId)">
            @Html.DisplayNameFor(X => X.UserId)
        </th>
        <th data-column-id="@nameof(Model.ClassTime)">
            @Html.DisplayNameFor(X => X.ClassTime)
        </th>
        <th data-column-id="@nameof(Model.SessionCount)">
            @Html.DisplayNameFor(X => X.SessionCount)
        </th>
        <th data-column-id="@nameof(Model.ClassSession)">
            @Html.DisplayNameFor(X => X.ClassSession)
        </th>
        <th data-column-id="@nameof(Model.StartDateSh)">
            @Html.DisplayNameFor(X => X.StartDateSh)
        </th>
        <th data-column-id="@nameof(Model.FinishDateSh)">
            @Html.DisplayNameFor(X => X.FinishDateSh)
        </th>
   
        <th data-column-id="commands" data-formatter="commands" data-sortable="false">
            @LocalMessage.Action
        </th>
    </tr>
</thead>
