        <th data-column-id="@nameof(Model.FullName)">
            @Html.DisplayNameFor(X => X.FullName)
        </th>
        <th data-column-id="@nameof(Model.Mobile)">
            @Html.DisplayNameFor(X => X.Mobile)
        </th>
        <th data-column-id="@nameof(Model.BirthDate)">
            @Html.DisplayNameFor(X => X.BirthDate)
        </th>
        <th data-column-id="@nameof(Model.RegisterDate)">
            @Html.DisplayNameFor(X => X.RegisterDate)
        </th>
        <th data-column-id="@nameof(Model.LastLoginDate)">
            @Html.DisplayNameFor(X => X.LastLoginDate)
        </th>
        <th data-column-id="@nameof(Model.IsTeacher)">
            @Html.DisplayNameFor(X => X.IsTeacher)
