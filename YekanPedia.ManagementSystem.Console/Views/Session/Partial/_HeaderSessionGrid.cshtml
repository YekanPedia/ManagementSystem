@model ClassSession
@{ Layout = null;}
<thead>
    <tr>
        <th data-column-id="Subject">
            @Html.DisplayNameFor(X => X.Subject)
        </th>
        <th data-column-id="Description">
            @Html.DisplayNameFor(X => X.Description)
        </th>
        <th data-column-id="ClassSessionDateSh">
            @Html.DisplayNameFor(X => X.ClassSessionDateSh)
        </th>
        <th data-column-id="IsCanceled">
            @Html.DisplayNameFor(X => X.IsCanceled)
        </th>
        <th>
            @LocalMessage.Session
        </th>
        <th>
            @LocalMessage.FilesCount
        </th>
        <th>
            @LocalMessage.Action
        </th>
    </tr>
</thead>
