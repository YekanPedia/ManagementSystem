@model UserInClass

<thead>
    <tr>
        <th data-column-id="@nameof(Model.Class.ClassInformaion)">
            @Html.DisplayNameFor(X => X.Class.ClassInformaion)
        </th>
        <th data-column-id="@nameof(Model.PaymentDate)">
            @Html.DisplayNameFor(X => X.PaymentDate)
        </th>
        <th data-column-id="@nameof(Model.Price)">
            @Html.DisplayNameFor(X => X.Price)
        </th>
        <th data-column-id="@nameof(Model.SessionCount)">
            @Html.DisplayNameFor(X => X.SessionCount)
        </th>
        <th data-column-id="@nameof(Model.TransactionCode)">
            @Html.DisplayNameFor(X => X.TransactionCode)
        </th>
        <th data-column-id="@nameof(Model.Class.IsFinished)">
            @Html.DisplayNameFor(X => X.Class.IsFinished)
        </th>
        <th data-column-id="@nameof(Model.IsFinished)">
            @Html.DisplayNameFor(X => X.IsFinished)
        </th>
        <th>
            @LocalMessage.Action
        </th>
    </tr>
</thead>

